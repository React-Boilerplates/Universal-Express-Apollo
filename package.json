    "cpu": [
        "x64",
        "ia32"
    ],
    "scripts": {
        "pretest": "node ./tools/jest/globalSetup.js",
        "lint:es": "eslint . --cache",
        "lint:styles": "stylelint .",
        "lint": "npm run lint:es && lint:styles",
        "test": "jest",
        "assets:windows": "echo {\"app\":{\"js\":\"/assets/app.js\"},\"vendor\":{\"js\":\"/assets/vendor.js\"}} > assets.json",
        "assets": "echo '{\"app\":{\"js\":\"/assets/app.js\"},\"vendor\":{\"js\":\"/assets/vendor.js\"}}' > assets.json",
        "test:silent": "jest --silent",
        "build:server": "cross-env NODE_ENV=production BABEL_ENV=server:prod webpack --mode production --config ./tools/webpack/production.server.js",
        "build:client": "cross-env NODE_ENV=production BABEL_ENV=client:prod webpack --mode production --config ./tools/webpack/production.client.js",
        "dev:client": "cross-env NODE_ENV=development BABEL_ENV=client webpack --mode development --config ./tools/webpack/development.client.js",
        "build": "npm run build:client && npm run build:server",
        "inspect:client-bundle": "cross-env NODE_ENV=production BABEL_ENV=client:prod webpack --mode production --config tools/webpack/production.client.js --json > client-stats.json",
        "inspect:server-bundle": "cross-env NODE_ENV=production BABEL_ENV=server:prod webpack --mode production --config tools/webpack/production.server.js --json > server-stats.json",
        "inspect:bundles": "npm run inspect:server-bundle && npm run inspect:client-bundle",
        "clean": "npm run clean:server && npm run clean:client",
        "clean:server": "rm -rf build",
        "clean:client": "rm -rf public",
        "pre:start": "npm run dev:client && npm run clean",
        "start": "cross-env NODE_ENV=development BABEL_ENV=server babel-node --harmony server/dev.js",
        "start:prod": "cross-env node build/main.js",
        "heroku-postbuild": "npm run build"
    },
    "bugs": {
        "url": "https://github.com/React-Boilerplates/Universal-Express-Apollo/issues"
